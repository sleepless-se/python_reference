        "import re\n",
        "regex = r'disable|soldout'\n",
        "isDisable = re.compile(regex)\n",
        "match = isDisable.search(\"disable Soldout\")\n",
        "if match:\n",
        "  print(\"Yes\")\n",
        "else:\n",
        "  print(\"No\")\n"
      ],
